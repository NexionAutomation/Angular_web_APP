{
  "name": "nexion",
  "version": "1.0.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "ng serve",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "update": "ng update --force @angular/cli @angular/core",
    "postinstall": "ngcc",
    "start:electron": "ng build --base-href ./ && electron .",
    "Deploy:App": "electron-packager APP Nexion --platform=win32 --arch=x64",
    "electron-package-win": "ng build --prod && electron-packager . --no-prune --ignore=/node_modules --ignore=/e2e --ignore=/src --overwrite --icon=./favicon.icon",
    "build_Pro": "ng build --prod"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ng lint && lint-staged"
    }
  },
  "dependencies": {
    "@angular/animations": "13.2.0",
