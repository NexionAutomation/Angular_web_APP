    "bootstrap": "^4.6.0",
    "chart.js": "^4.0.1",
    "chartjs-plugin-datalabels": "^0.6.0",
    "core-js": "2.5.1",
    "datatables.net": "^1.12.0",
    "datatables.net-buttons": "^2.2.3",
    "datatables.net-buttons-dt": "^2.2.3",
    "datatables.net-colreorder": "^1.5.6",
